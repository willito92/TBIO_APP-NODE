{{#each errors}}
<div class="alert alert-success dismissible fade show role=" alert"">
    {{text}}
    <button type="button" class="close" data-dismiss="danger" aria-label="close">
        <span aria-hidden="true">&times;</span>
    </button>


</div>
{{/each}}

{{#if error}}
<div class="alert alert-success dismissible fade show role=" alert"">
    {{error}}
    <button type="button" class="close" data-dismiss="danger" aria-label="close">
        <span aria-hidden="true">&times;</span>
    </button>


</div>
{{/if}}